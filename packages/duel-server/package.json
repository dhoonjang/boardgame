{
  "name": "@duel/server",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./game": {
      "import": "./dist/game/index.js",
      "types": "./dist/game/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/game/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch --onSuccess 'node dist/index.js'",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@hono/node-server": "^1.8.0",
    "dotenv": "^17.2.4",
    "hono": "^4.0.0",
    "socket.io": "^4.7.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "socket.io-client": "^4.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
